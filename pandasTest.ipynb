   "source": [
    "import pandas as pd\n",
    "\n",
    "# 예시 데이터 생성\n",
    "data = {\n",
    "    'Category': ['A', 'B', 'A', 'B', 'A'],\n",
    "    'Value': [10, 20, 30, 40, 50]\n",
    "}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# 'Category' 열을 기준으로 그룹화하고 각 그룹에 대해 평균을 계산\n",
    "result = df.groupby('Category').mean()\n",
    "\n",
    "print(result)\n"
   ]
  }
